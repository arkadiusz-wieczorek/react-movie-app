    "build": "browserify ./lib/app.jsx -o ./build/js/bundle.js -t [ babelify --presets [ es2015 react ] ] -v",
    "watch": "watchify ./lib/app.jsx -o ./build/js/bundle.js -t [ babelify --presets [ es2015 react ] ] -v",
    "build-production": "browserify ./lib/app.jsx -o ./build/js/bundle.js -t [ babelify --presets [ es2015 react ] ]  -g [ envify --NODE_ENV production ] -g uglifyify -p bundle-collapser/plugin --compress --mangle",
    "start": "cd ./build && python -m SimpleHTTPServer 8888"
